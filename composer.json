{
    "version": "1.0.0",
    "name": "crucialdigital/metamorph",
    "description": "Package of data models managements",
    "keywords": [
        "crucialdigital",
        "laravel",
        "metamorph",
        "mongodb",
        "crud",
        "model"
    ],
    "homepage": "https://github.com/crucialdigital/metamorph",
    "license": "MIT",
    "authors": [
        {
            "name": "Humbert DJAGLO",
            "email": "djak1223@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|8.2",
        "illuminate/contracts": "^8|^9|^10|^11",
        "illuminate/support": "^8|^9|^10|^11",
        "intervention/image": "^2.7",
        "maatwebsite/excel": "^3.1",
        "mongodb/laravel-mongodb": "^4.2",
        "psr/simple-cache": "^2.0|^3.0",
        "spatie/laravel-package-tools": "^1.13.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.1",
        "larastan/larastan": "^2.0.1",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.34",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "CrucialDigital\\Metamorph\\": "src",
            "CrucialDigital\\Metamorph\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CrucialDigital\\Metamorph\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CrucialDigital\\Metamorph\\MetamorphServiceProvider"
            ],
            "aliases": {
                "Metamorph": "Metamorph"
            }
        },
        "branch-alias": {
            "dev-dev": "1.0.x-dev",
            "dev-master": "master-dev"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
